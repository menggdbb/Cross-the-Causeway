<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tehosiewdai.gojbboh">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.SplashActivity"></activity>
        <activity android:name=".activities.WoodlandsTrafficImageActivity" />
        <activity android:name=".activities.TuasTrafficImageActivity" />
        <activity android:name=".activities.MoneyChangerActivity" />
        <activity android:name=".activities.CurrencyActivity" />
        <activity android:name=".activities.CalendarActivity" />
        <activity
            android:name=".activities.MenuActivity"
            android:label="@string/menu"
            android:parentActivityName=".activities.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.MainActivity" />
        </activity>
        <activity
            android:name=".activities.MainActivity"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>